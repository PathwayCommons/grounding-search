{
  "name": "grounding-search",
  "version": "0.0.0",
  "description": "Biological entity grounding search service",
  "repository": {
    "type": "git",
    "url": "https://github.com/PathwayCommons/grounding-search.git"
  },
  "bugs": {
    "url": "https://github.com/PathwayCommons/grounding-search/issues"
  },
  "homepage": "https://github.com/PathwayCommons/grounding-search",
  "scripts": {
    "start": "node ./src/server",
    "inspect": "node --inspect-brk ./src/server",
    "lint:js": "eslint ./src",
    "lint": "run-s lint:*",
    "watch": "nodemon src/server/index.js",
    "test": "mocha",
    "test:temp": "mocha test/temp",
    "test:sample": "cross-env TESTS_BUILD_INDEX=true INPUT_PATH=test/input/ INDEX=graundingsearch-test mocha",
    "test:temp:sample": "cross-env TESTS_BUILD_INDEX=true INPUT_PATH=test/input/ INDEX=graundingsearch-test mocha test/temp",
    "benchmark": "run-s benchmark:*",
    "benchmark:uniprot": "node benchmark/uniprot",
    "benchmark:chebi": "node benchmark/chebi",
    "benchmark:ncbi": "node benchmark/ncbi",
    "update": "run-p update:*",
    "update:uniprot": "node src/server/datasource/cli update uniprot",
    "update:chebi": "node src/server/datasource/cli update chebi",
    "update:ncbi": "node src/server/datasource/cli update ncbi",
    "clear": "run-p clear:*",
    "clear:uniprot": "node src/server/datasource/cli clear uniprot",
    "clear:chebi": "node src/server/datasource/cli clear chebi",
    "clear:ncbi": "node src/server/datasource/cli clear ncbi",
    "ci": "run-s test:sample lint"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "binary-split": "^1.0.5",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "dice-coefficient": "^1.0.3",
    "download": "^7.1.0",
    "elasticsearch": "^15.4.1",
    "express": "^4.13.4",
    "fibers": "^3.1.1",
    "ftp": "^0.3.10",
    "lodash": "^4.17.11",
    "morgan": "^1.7.0",
    "saxes": "^3.1.9",
    "serve-favicon": "^2.3.0",
    "swagger-jsdoc": "^3.2.7",
    "swagger-ui-express": "^4.0.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "forever": "^0.15.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "xml-js": "^1.6.11"
  },
  "engines": {
    "node": ">=8.0"
  }
}
