  "version": "0.2.0",
  "configurations": [

    
    {
      "type": "node",
      "request": "launch",
      "name": "Launch server",
      "runtimeArgs": ["-r", "esm"],
      "program": "${workspaceFolder}/src/server/index.js"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Debug NCBI update",
      "runtimeArgs": ["-r", "esm"],
      "program": "${workspaceFolder}/src/server/datasource/cli.js",
      "args": ["update", "ncbi"]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Debug Chebi update",
      "runtimeArgs": ["-r", "esm"],
      "program": "${workspaceFolder}/src/server/datasource/cli.js",
      "args": ["update", "chebi"]
    }
  ]
}